<Window x:Class="ImageDownloader.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageDownloader.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <TextBox x:Name="urlTextBox" DockPanel.Dock="Top"></TextBox>
        <Button x:Name="button" Click="Button_Click" DockPanel.Dock="Top"></Button>
        <StatusBar x:Name="statusBar" DockPanel.Dock="Bottom"></StatusBar>
        <WrapPanel x:Name="wrapPanel" DockPanel.Dock="Bottom"></WrapPanel>
    </DockPanel>
</Window>
